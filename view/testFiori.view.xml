							<ScrollContainer horizontal="false">
								<VBox>
									<ScrollContainer vertical="false">
										<HBox >
											<l:VerticalLayout width="100%" class="gridWrapper">
												<l:Grid containerQuery="true">
													<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom tileLayout " headerImage="sap-icon://sales-order-item" mode="ContentMode" header="Inwarding"
														press="press" visible="{mMainModel>/VisibleTiles/GrpIn}" >
														<TileContent >
															<VBox height="100%">
																<!--<Link emphasized="true" class="GenericTileCustom"  text="Link 1" class="" emphasized="true" press="onPressed"/>-->
																<!--<GenericTile mode="LineMode" class="GenericTileCustom" header="Update software" press="press"-->
																<!--	visible="{mMainModel>/VisibleTiles/InUpdtSoftware}"/>-->
																<Link emphasized="true" class="GenericTileCustom"  text="Inward of asset"  press="onInwardofAsset" visible="{mMainModel>/VisibleTiles/InAst}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Inward of accessory" press="onInwardOfAccessorySem" visible="{mMainModel>/VisibleTiles/InAsc}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Update accessory details" press="onUpdateAccessorySem" visible="{mMainModel>/VisibleTiles/InUpdateAsc}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Add software" press="press" visible="{mMainModel>/VisibleTiles/InAddSoft}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Update software" press="press" visible="{mMainModel>/VisibleTiles/InUpdtSoftware}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Inward IO/Non PO" press="onNonPOAsset" visible="{mMainModel>/VisibleTiles/OptChar1}"/>
															</VBox>
														</TileContent>
													</GenericTile>
													<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom tileLayout" headerImage="sap-icon://learning-assistant" header="Internal Process" press="press"
														visible="{mMainModel>/VisibleTiles/GrpInt}">
														<TileContent>
															<VBox width="100%">
																<Link emphasized="true" class="GenericTileCustom"  text="Common dashboard" press="onCommonDBSem" width="100%"
																	visible="{mMainModel>/VisibleTiles/IntAstCommDashboard}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Allocation Non-Assets/Accesories" width="100%" press="onAllocNonAsseSem"
																	visible="{mMainModel>/VisibleTiles/IntAlloNonAstAsc}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Accesory De-Allocation" press="press"
																	visible="{mMainModel>/VisibleTiles/IntAscDeallocation}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Action Item" press="onActionItem"
																	visible="{mMainModel>/VisibleTiles/TransActionItem}"/>
															</VBox>
														</TileContent>
													</GenericTile>
													<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom tileLayout" headerImage="sap-icon://org-chart" header="Inter/Intra Transfer" press="press"
														visible="{mMainModel>/VisibleTiles/GrpTrans}">
														<TileContent>
															<VBox>
																<Link emphasized="true" class="GenericTileCustom"  text="Accessory Transfer" press="press"
																	visible="{mMainModel>/VisibleTiles/TransAscTransfer}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Receive Gatepass" press="onSearchGatepassSem"
																	visible="{mMainModel>/VisibleTiles/TransReceive}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Download/Print Gatepass" press="press"
																	visible="{mMainModel>/VisibleTiles/TransPrintGatepass}"/>
															</VBox>
														</TileContent>
													</GenericTile>
													<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom tileLayout" headerImage="sap-icon://add-coursebook" header="Physical Inventory" press="press"
														visible="{mMainModel>/VisibleTiles/GrpPi}">
														<TileContent>
															<VBox>
																<Link emphasized="true" class="GenericTileCustom"  text="Create PI Assets" press="onCreatePIAssets"
																	visible="{mMainModel>/VisibleTiles/PiCreat}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Display/Edit Physical Inventory" press="onDisplayEditPI"
																	visible="{mMainModel>/VisibleTiles/PiDisplayEdit}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Physical Inventory Report" press="onPhysicalInventoryReport"
																	visible="{mMainModel>/VisibleTiles/PiReport}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Physical Verification" press="onPhysicalVerification"
																	visible="{mMainModel>/VisibleTiles/OptChar4}"/>
															</VBox>
														</TileContent>
													</GenericTile>
													<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom tileLayout" headerImage="sap-icon://create-form" header="Reports" press="press"
														visible="{mMainModel>/VisibleTiles/GrpRep}">
														<TileContent>
															<VBox>
																<Link emphasized="true" class="GenericTileCustom"  text="Asset Report" press="onAssetReport"
																	visible="{mMainModel>/VisibleTiles/RepTransfer}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Accessory Report" press="onAccessoryReport"
																	visible="{mMainModel>/VisibleTiles/RepInwarding}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Software Report" press="onSoftwareReport"
																	visible="{mMainModel>/VisibleTiles/RepInternalProcess}"/>
																<Link emphasized="true" class="GenericTileCustom"  text="Mass Tag Printing" press="onMassTagPrinting"
																	visible="{mMainModel>/VisibleTiles/OptChar3}"/>
																	<Link emphasized="true" class="GenericTileCustom"  text="Asset Graphical Report" press="onAssetGraphicalReport"
																	visible="{mMainModel>/VisibleTiles/OptChar2}"/>
															</VBox>
														</TileContent>
													</GenericTile>
												</l:Grid>
											</l:VerticalLayout>
										</HBox>
									</ScrollContainer>
								</VBox>

						</Page>
					</l:SplitPane>
					<l:PaneContainer orientation="Vertical" resize=".onInnerContainerResize">
						<l:SplitPane requiredParentWidth="1100">
							<l:layoutData>
								<l:SplitterLayoutData size="{sizes>/pane2}"/>
							</l:layoutData>
							<Page title="ACTION ITEMS">
								<headerContent>
									<OverflowToolbar id="actionItemTableOverflowTool" visible="true">
										<ToolbarSpacer></ToolbarSpacer>
										<Button tooltip="Export" icon="sap-icon://excel-attachment" press="onExport1" visible="true"/>
										<Button tooltip="sort" icon="sap-icon://sort" press="onSort1" visible="true"/>
										<Input visible="true" liveChange="onSearchTable1" id="hideableInput" width="60%"/>
									</OverflowToolbar>
								</headerContent>
								<Text text="LayoutData.size={sizes>/pane3}"/>
								<Table width="100%" id="ActionItemTabId" autoPopinMode="true" items="{ path:'mMainModel>/table1'}"
									sticky="HeaderToolbar,InfoToolbar,ColumnHeaders" growing="true" growingScrollToLoad="true" growingThreshold="10">
									<columns >
										<Column width="6rem" hAlign="Center">
											<Label text="Action" design="Bold"/>
										</Column>
										<Column width="6rem" hAlign="Center">
											<Label text="Request No" design="Bold"/>
										</Column>
										<Column width="10rem" hAlign="Center">
											<Label text="Asset"  design="Bold" wrapping= "false"/>
										</Column>
										<Column width="4rem" hAlign="Center">
											<Label text="Status" design="Bold"/>
										</Column>
										<Column width="6rem" hAlign="Center">
											<Label text="Request Date" design="Bold" wrapping="true"/>
										</Column>
										<!--<Column width="6rem" hAlign="Center" >-->
										<!--	<Label text="Pending with" design="Bold" wrapping="true"/>-->
										<!--</Column>-->
									</columns>
									<items>
										<ColumnListItem type="Navigation" press="onPressActionItem">
											<cells>
												<Text text="{mMainModel>RequestTypeDesc}"></Text>
												<Text text="{mMainModel>RequestNo}"/>
												<Text text="{mMainModel>AssetName}" wrapping="false"/>
												<Text text="{mMainModel>StatusText}"></Text>
												<Text text="{path :'mMainModel>RequestDate', formatter:'com.maventic.asset.Home.utility.Formatter.ConvDateforComments'}"/>
												<!--<Text text="{mMainModel>CrName}"></Text>-->
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</Page>
						</l:SplitPane>
						<l:SplitPane requiredParentWidth="1000">
							<l:layoutData>
								<l:SplitterLayoutData size="{sizes>/pane3}"/>
							</l:layoutData>
							<Page title="SAP NOTIFICATION">
								<headerContent>
									<OverflowToolbar id="actionItemTableOverflowTool2" visible="true">
										<ToolbarSpacer></ToolbarSpacer>
										<Button tooltip="Export" icon="sap-icon://excel-attachment" press="onExport2" visible="true"/>
										<Button tooltip="sort" icon="sap-icon://sort" press="onSort2" visible="true"/>
										<Input visible="true" liveChange="onSearchTable2" width="60%"/>
									</OverflowToolbar>
								</headerContent>
								<Text text="LayoutData.size={sizes>/pane3}"/>
								<Table width="100%" items="{ path:'mMainModel>/table2'}" id="NotificationTabId" autoPopinMode="true"
									sticky="HeaderToolbar,InfoToolbar,ColumnHeaders" growing="true" growingScrollToLoad="true" growingThreshold="10">
									<columns >
										<Column width="6rem">
											<Label text="Request No" design="Bold"/>
										</Column>
										<Column width="6rem">
											<Label text="Asset/Accessory" design="Bold"/>
										</Column>
										<Column width="6rem">
											<Label text="Notification" design="Bold"/>
										</Column>
									</columns>
									<items>
										<ColumnListItem >
											<cells>
												<Text text="{mMainModel>RequestNo}"/>
												<Text text="{mMainModel>AssetName}"/>
												<Text text="{mMainModel>Notification}"></Text>
											</cells>
										</ColumnListItem>
									</items>
								</Table>
							</Page>
						</l:SplitPane>
					</l:PaneContainer>
				</l:PaneContainer>
			</l:ResponsiveSplitter>
		</content>
	</Page>
